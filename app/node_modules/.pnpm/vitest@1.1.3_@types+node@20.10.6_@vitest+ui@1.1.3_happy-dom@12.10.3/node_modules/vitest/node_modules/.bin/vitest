#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/louisknolle/ps-parent/app/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_@vitest+ui@1.1.3_happy-dom@12.10.3/node_modules/vitest/node_modules:/Users/louisknolle/ps-parent/app/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_@vitest+ui@1.1.3_happy-dom@12.10.3/node_modules:/Users/louisknolle/ps-parent/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/louisknolle/ps-parent/app/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_@vitest+ui@1.1.3_happy-dom@12.10.3/node_modules/vitest/node_modules:/Users/louisknolle/ps-parent/app/node_modules/.pnpm/vitest@1.1.3_@types+node@20.10.6_@vitest+ui@1.1.3_happy-dom@12.10.3/node_modules:/Users/louisknolle/ps-parent/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
